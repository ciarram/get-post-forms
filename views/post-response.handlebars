<h1>POST Request Received</h1>

<table>
    <thead>
      <tr>URL Query Results</tr>
      <tr>
        <th>Name</th>
        <th>Value</th>
      </tr>
    </thead>

    <tbody>
        {{#each dataList}}
            <tr>
                <td>{{this.data}}</td>

                <td>{{this.result}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

<table>
  <thead>
    <tr>JSON Data Results</tr>
    <tr>
        <th>Name</th>
        <th>Value</th>
    </tr>
  </thead>
  <tbody>
    {{#each dataList}}
      <tr>
        <td>{{this.data}}</td>
        <td>{{this.result}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

<ul>
  {{#each dataList}}
    <li>{{this.name}}: {{this.value}}
  {{/each}}
</ul>